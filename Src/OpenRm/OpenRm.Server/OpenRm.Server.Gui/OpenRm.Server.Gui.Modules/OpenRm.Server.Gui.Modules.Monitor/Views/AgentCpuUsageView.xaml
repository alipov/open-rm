<Window x:Class="OpenRm.Server.Gui.Modules.Monitor.Views.AgentCpuUageView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"    
    Title="CPU usage view" Height="180 " Width="350" Loaded="AgentCpuUageView_Loaded">

    <d3:ChartPlotter Name="CpuPlotter">
        <d3:Header TextBlock.FontSize="14">
            CPU Usage
        </d3:Header>

        <d3:VerticalAxisTitle TextBlock.FontSize="10">CPU %</d3:VerticalAxisTitle>
        <d3:HorizontalAxisTitle TextBlock.FontSize="10">Time</d3:HorizontalAxisTitle>
    </d3:ChartPlotter>
</Window>


